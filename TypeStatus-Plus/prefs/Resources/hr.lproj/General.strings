<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Header of status bar section. -->
	<key>STATUS_BAR</key>
	<string>Statusna traka</string>

	<!-- Switch to toggle displaying the unread count in the status bar. -->
	<key>UNREAD_COUNT</key>
	<string>Broj nepročitanih</string>

	<!-- Button for choosing from which app the unread notifications count will be shown in the status bar. -->
	<key>SELECT_APPS</key>
	<string>Odaberi aplikacije</string>

	<!-- Explanation of the unread notifications count button. -->
	<key>UNREAD_COUNT_EXPLANATION</key>
	<string>Prikazuje broj nepročitanih obavijesti označenih aplikacija u statusnoj traci.</string>
</dict>
</plist>
